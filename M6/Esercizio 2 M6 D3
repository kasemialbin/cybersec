.MODEL SMALL
.STACK 100H

.DATA
a DW 10
b DW 5

.CODE
MAIN PROC
    MOV AX, @DATA
    MOV DS, AX

    ; Carica il valore di 'a' in AX
    MOV AX, [a]

    ; Aggiunge 3 a 'a'
    ADD AX, 3

    ; Moltiplica 'a' per il valore di 'b'
    MOV BX, [b]
    MUL BX

    ; Il risultato Ã¨ ora nel registro DX:AX
    ; Memorizziamo il risultato nel registro accumulatore (AL)
    MOV AL, AL

    ; Fine del programma
    MOV AH, 4CH
    INT 21H

MAIN ENDP
END MAIN
